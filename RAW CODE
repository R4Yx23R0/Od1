    Console.WriteLine("......::++++:++++::+*##################");
    Console.WriteLine(".......::::::::::::......-+#@@@@@@@@@##");
    Console.WriteLine("........-:::-::::-............-:+=#@@@#");
    Console.WriteLine("....................................-*#");
}

